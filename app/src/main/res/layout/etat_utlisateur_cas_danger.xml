<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="405dp"
        android:layout_height="177dp"
        app:srcCompat="@drawable/images"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="2dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="402dp"
        android:layout_height="128dp"
        android:text="votre pourcentage d'infection au coronavirus est :"
        android:textColor="#B60A0A"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="198dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="303dp"
        android:layout_height="49dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=" score                                               %"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="341dp" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="248dp"
        android:layout_height="181dp"
        app:srcCompat="@drawable/smile_danger"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="403dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="350dp"
        android:layout_height="109dp"
        android:text="En se basant sur votre évaluation récente de votre état de santé, il est fort possible que vous etes infecté au coronavirus .             Veuillez passer un test  dans les 24 heures"
        android:textColor="#B60A0A"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="609dp" />

</androidx.constraintlayout.widget.ConstraintLayout>